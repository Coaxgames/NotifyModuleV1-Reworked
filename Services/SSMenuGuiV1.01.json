--TAG;/n{"UGUISystem":{"DestinationParent":"StarterCharacterScripts","NME":"UGUISystem","Properties":{"Enabled":"true","LinkedSource":"","Disabled":"false","Parent":"ProjectFolder","Name":"UGUISystem","Archivable":"true","Source":"local Settings = script:WaitForChild(\"Settings\")\nlocal Assets = script:WaitForChild(\"Assets\")\nlocal PlayerWS = script.Parent\nwarn(PlayerWS)\n--Script Settings (HardCoded)\nlocal DL = { --Debug Stuff\n\t[\"Startup\"]=false,\n\t[\"PatchWork\"]=false,\n\t[\"ButtonComTX\"]=false,\n\t[\"ButtonComRX\"]=true,\n\t[\"Errors\"]=true,\n}\n\n\n--Setup Player\nlocal Player = game.Players:FindFirstChild(PlayerWS.Name)\nlocal Players = {}\nfor i, Obj in game.Players:GetChildren() do\n\tPlayers[Obj.Name]=Obj\nend\nif Players[PlayerWS.Name]~=nil then\n\tPlayer = game.Players[PlayerWS.Name]\n\twarn(\"Found Player\")\nelse\n\twarn(\"Error finding Player\")\nend\nlocal Player = game.Players[PlayerWS.Name]\nlocal Humanoid = script.Parent:WaitForChild(\"HumanoidRootPart\")\nlocal RSM = Assets:FindFirstChild(\"PluginService\")\nlocal GuiService = script.Coms:FindFirstChild(\"TempCom\")\n--Inject a service on the Settings menu\nlocal InjectMenuItem = coroutine.create(function(MenuName, MenuProps, ExtraData)\n\t\nend)\nif RSM==nil then\n\t--coroutine.resume(InjectMenuItem)\nend\n\n\n\n\n\n--Create GUI Setting panel\nlocal NewData = Instance.new(\"Folder\")\nNewData.Parent = script:FindFirstChild(\"Settings\")\nNewData.Name = \"GUI Settings\"\n\n--Get the player GUI's that have a settings folder with them, i just havnt piped this to the builder\nwarn(\"Building Setting Icons\")\nlocal BuildTable={}\nlocal SettingsToAdd = {}\nlocal PlayerGUIs = Player.PlayerGui\nfor i, Obj in pairs(PlayerGUIs:GetChildren()) do\n\twarn(Obj)\n\twarn(Obj.Name)\n\tlocal TMP = Obj:FindFirstChild(\"Settings\")\n\tif TMP~=nil then\n\t\twarn(\"DB1\")\n\t\tlocal TMP1 = TMP:GetChildren()\n\t\tfor i, Obj2 in pairs(TMP1) do\n\t\t\tSettingsToAdd[Obj] = Obj2:Clone()\n\t\t\tSettingsToAdd[Obj].Parent = NewData\t\n\t\tend\n\t\t\n\tend\nend\n\n\n--DevNote#2#2#\n--Button Manager for External Use(Pipe this to external scripts, with a new event) ~Finished\nif script:FindFirstChild(\"Coms\") then\n\t--fire to button handle\n\tif script.Coms:FindFirstChild(\"ExternCom\") then\n\t\tscript.Coms.ExternCom.Event:Connect(function(Plr, ButtonName, DP, DB)\n\t\t\tif DB==true then\n\t\t\t\t--warn(ButtonName, \", Error Internal Warning: \", \"0x0421\", \"| (DB=\", DB, \"), DB is not false creating a Feedback loop issue\")\n\t\t\t\treturn\n\t\t\tend\n\t\t\t\n\t\t\t--CreateIcon Commanded structure(lazy and bad, re-write when possible)\n\t\t\tif DP[\"Command\"] then\n\t\t\t\tif DP[\"Command\"]==\"CreateIcon\" then\n\t\t\t\t\tif DP[\"Folder\"] then\n\t\t\t\t\t\tif DP[\"GUITarget\"] then\n\t\t\t\t\t\t\tif DP[\"CallbackName\"] then\n\t\t\t\t\t\t\t\tCreateNewElement(DP[\"Folder\"], DP[\"GUITarget\"], DP[\"CallbackName\"])\n\t\t\t\t\t\t\t\treturn\n\t\t\t\t\t\t\tend\n\t\t\t\t\t\tend\n\t\t\t\t\tend\n\t\t\t\tend\n\t\t\tend\n\t\t\t\n\t\t\t--Regular Coms\n\t\t\tif Plr==nil then\n\t\t\t\twarn(ButtonName, \", Pass Error: Plr is nil\", \"| Plr is nil! cannot send to local player...\")\n\t\t\t\treturn\n\t\t\telse\n\t\t\t\tif DB==false then\n\t\t\t\t\twarn(Plr, \"Pressed: \", ButtonName, \", Datapacket =\", DP, \", DB =\", DB)\n\t\t\t\t\tscript.Coms.GuiComs:FireClient(Plr, ButtonName, DP, DB)--Fire back for now with DB as false\n\t\t\t\telse\n\t\t\t\t\twarn(ButtonName, \", Error Internal error: \", \"0x4421\")\n\t\t\t\tend\n\t\t\tend\n\t\tend)\n\tend\n\t--Fire to extern event\n\tif script.Coms:FindFirstChild(\"GuiComs\") then\n\t\tscript.Coms.GuiComs.OnServerEvent:Connect(function(Plr, ButtonName, DP, DB)\n\t\t\tif Plr==nil then\n\t\t\t\twarn(Plr, \"Pressed: \", ButtonName, \", Datapacket =\", DP, \"| Plr is NIL\")\n\t\t\t\treturn\n\t\t\telse\n\t\t\t\twarn(Plr, \"Pressed: \", ButtonName, \", Datapacket =\", DP, \"| Sending to Extern\")\n\t\t\t\tscript.Coms.ExternCom:Fire(Plr, ButtonName, DP, true)--Fire to Extern\n\t\t\tend\n\t\tend)\n\tend\nend\n\n\n\n\n--Build function, Finished (Without testing)\nfunction CreateNewElement(Folder : \"The pointer, can be set to anything as long as the Data is there\", ScreenGui : \"The Target GUI destination (Default==navbar)\", CallbackName : \"the name of the script that called this\")\n\twarn(Folder)\n\tif Folder==nil then\n\t\tFolder = Settings\n\tend\n\tif ScreenGui==nil then\n\t\tScreenGui = Player.PlayerGui:FindFirstChild(\"MenuScreen\")\n\tend\n\tBuildTable={} --reset build table\n\t\n\t--Build Menu(need to add a way for external scripts to create these icons and setup a callback)\n\tfor i0, Obj in ipairs(Folder:GetChildren()) do\n\t\tif Obj.ClassName == \"Folder\" then\n\t\t\t--Add Interaction Buttons ans stuff to here\n\t\t\tlocal NewOverlay = Assets:FindFirstChild(\"Icons\"):FindFirstChild(\"OverlayIcon\"):Clone() \n\t\t\tNewOverlay.Name=Obj.Name..\"OverLay\" --Change Overlay name to Settingname\n\t\t\tNewOverlay.Parent = script:FindFirstChild(\"Build\")\n\n\t\t\t--Add Interaction MENU for those Buttons Overlay\n\t\t\tlocal NewNavBarIcon = Assets:FindFirstChild(\"Icons\"):FindFirstChild(\"NavBarIcon\"):Clone()\n\t\t\tNewNavBarIcon.Name = Obj.Name..\"Menu\" --Used foe Targeting the Overlay for X Setting\n\t\t\tNewNavBarIcon.Interaction.TitleCard.Text = Obj.Name --Change the Dispaleyd Overlay name to Settingname\n\t\t\tNewNavBarIcon.Parent = script:FindFirstChild(\"Build\")\n\n\t\t\tlocal StateVar0 = Assets.Scripts:FindFirstChild(\"Attributes\"):Clone()\n\n\t\t\t--Set Display Text Then Add to build table\n\t\t\tBuildTable[Obj.Name]={\n\t\t\t\t[NewOverlay.Name]=NewOverlay, --Icons go here\n\t\t\t\t[NewNavBarIcon.Name]=NewNavBarIcon, --Menu Icon itself\n\t\t\t}\n\t\t\t\n\t\t\t--patch function, added for adding attributes or patching in functions i missed\n\t\t\tlocal function ApplyPatch(Obj, AtrName, Atr)\n\t\t\t\t--Patch in\n\t\t\t\tObj:SetAttribute(AtrName, Atr)\n\t\t\t\tObj:SetAttribute(AtrName, Atr)\n\t\t\t\treturn\n\t\t\tend\n\n\n\t\t\t--Setup MAIN Menu Icon Script(Test this)\n\t\t\tif BuildTable[Obj.Name][NewNavBarIcon.Name]:FindFirstChild(\"ButtonScriptV2\") then\n\t\t\t\tlocal Script2 = BuildTable[Obj.Name][NewNavBarIcon.Name]:FindFirstChild(\"ButtonScriptV2\")\n\t\t\t\tStateVar0.Parent =Script2\n\t\t\t\tStateVar0:SetAttribute(\"SpecificName\", Obj.Name) --Set Attribute For naming\n\t\t\t\tApplyPatch(Script2, \"CallbackName\", CallbackName)\n\t\t\t\tApplyPatch(Script2, \"NavbarName\", NewNavBarIcon.Name)\n\t\t\t\tApplyPatch(Script2, \"OverlayName\", NewOverlay.Name)\n\t\t\t\tApplyPatch(Script2, \"SpecificName\", Obj.Name)\n\t\t\t\tScript2.Enabled = true\n\t\t\telse\n\t\t\t\tlocal Script2 = Assets.Scripts:FindFirstChild(\"ButtonScriptV2\")\n\t\t\t\tif Script2==nil then\n\t\t\t\t\tNewNavBarIcon.BackgroundColor3 = Color3.new(0.505882, 0.0470588, 0.0470588)\n\t\t\t\telse\n\t\t\t\t\tlocal TMP = Script2:Clone() --Create New From Assets\n\t\t\t\t\tTMP.Parent = BuildTable[Obj.Name][NewNavBarIcon.Name]\n\t\t\t\t\tStateVar0.Parent = TMP\n\t\t\t\t\tStateVar0:SetAttribute(\"SpecificName\", Obj.Name) --Set Attribute For naming\n\t\t\t\t\tApplyPatch(TMP, \"CallbackName\", CallbackName)\n\t\t\t\t\tApplyPatch(TMP, \"NavbarName\", NewNavBarIcon.Name)\n\t\t\t\t\tApplyPatch(TMP, \"OverlayName\", NewOverlay.Name)\n\t\t\t\t\tApplyPatch(TMP, \"SpecificName\", Obj.Name)\n\t\t\t\t\tScript2 = TMP\n\t\t\t\t\tScript2.Enabled= true\n\t\t\t\tend\n\t\t\tend\n\t\t\twarn(\"Folder: \"..Obj.Name)\n\n\t\t\t--Create Icons for setting\n\t\t\tfor i, Setting1 in ipairs(Obj:GetChildren()) do\n\t\t\t\tlocal NewIcon\n\t\t\t\tlocal StateVar = Setting1:Clone()\n\t\t\t\tlocal Script = nil\n\t\t\t\t--Create Icon Type\n\t\t\t\tif Setting1.ClassName==\"BoolValue\" then\n\t\t\t\t\tNewIcon = Assets:FindFirstChild(\"Icons\"):FindFirstChild(\"BoolIcon\"):Clone()\n\t\t\t\t\tNewIcon.Type.Value = \"Bool\"\n\t\t\t\t\tif Setting1.Value == true  then\n\t\t\t\t\t\tNewIcon.BoolState.BackgroundColor3=Color3.new(0,1,0)\n\t\t\t\t\telse\n\t\t\t\t\t\tNewIcon.BoolState.BackgroundColor3=Color3.new(1,0,0)\n\t\t\t\t\tend\n\t\t\t\telseif Setting1.ClassName==\"NumberValue\" then\n\t\t\t\t\tNewIcon = Assets:FindFirstChild(\"Icons\"):FindFirstChild(\"NumberIcon\"):Clone()\n\t\t\t\t\tNewIcon.Type.Value = \"Number\"\n\t\t\t\telseif Setting1.ClassName==\"StringValue\" then\n\t\t\t\t\tNewIcon = Assets:FindFirstChild(\"Icons\"):FindFirstChild(\"StringIcon\"):Clone()\n\t\t\t\t\tNewIcon.Type.Value = \"String\"\n\t\t\t\tend\n\n\t\t\t\t--Finish Icon Settings\n\t\t\t\tNewIcon.Name = Setting1.Name --Icon Target Name\n\t\t\t\tStateVar.Parent = NewIcon --Varible Reperent\n\t\t\t\tNewIcon.SettingTitle.Text = Setting1.Name --Display Name\n\n\n\n\n\t\t\t\t--Set icon into Destination\n\t\t\t\tNewIcon.Parent = BuildTable[Obj.Name][NewOverlay.Name].Buttons\n\t\t\t\tif ScreenGui then\n\t\t\t\t\tBuildTable[Obj.Name][NewOverlay.Name].Parent = ScreenGui:FindFirstChild(\"Overlays\")\n\t\t\t\t\tBuildTable[Obj.Name][NewNavBarIcon.Name].Parent = ScreenGui:FindFirstChild(\"NavBar\")\n\n\t\t\t\t\t--Set Vis\n\t\t\t\t\tBuildTable[Obj.Name][NewOverlay.Name].Visible=false\n\n\t\t\t\t\t--Setup Script for Icon Sub-Buttons(Test this)\n\t\t\t\t\tif NewIcon:FindFirstChild(\"ButtonScriptV2\") then\n\t\t\t\t\t\tStateVar.Parent = NewIcon:FindFirstChild(\"ButtonScriptV2\")\n\t\t\t\t\t\tStateVar:SetAttribute(\"SpecificName\", StateVar.Name) --Set Attribute For naming\n\t\t\t\t\t\tApplyPatch(NewIcon:FindFirstChild(\"ButtonScriptV2\"), \"SpecificName\", StateVar.Name)\n\t\t\t\t\t\tApplyPatch(NewIcon:FindFirstChild(\"ButtonScriptV2\"), \"NavbarName\", NewNavBarIcon.Name)\n\t\t\t\t\t\tApplyPatch(NewIcon:FindFirstChild(\"ButtonScriptV2\"), \"OverlayName\", NewOverlay.Name)\n\t\t\t\t\t\tApplyPatch(NewIcon:FindFirstChild(\"ButtonScriptV2\"), \"CallbackName\", CallbackName)\n\t\t\t\t\t\tNewIcon:FindFirstChild(\"ButtonScriptV2\").Enabled = true\n\t\t\t\t\telse\n\t\t\t\t\t\tScript = Assets.Scripts:FindFirstChild(\"ButtonScriptV2\")\n\t\t\t\t\t\tif Script==nil then\n\t\t\t\t\t\t\tNewIcon.BackgroundColor3 = Color3.new(0.505882, 0.0470588, 0.0470588)\n\t\t\t\t\t\telse\n\t\t\t\t\t\t\tlocal TMP = Script:Clone() --Create New From Assets\n\t\t\t\t\t\t\tTMP.Parent = NewIcon\n\t\t\t\t\t\t\tStateVar.Parent = TMP\n\t\t\t\t\t\t\tStateVar:SetAttribute(\"SpecificName\", StateVar.Name) --Set Attribute For naming\n\t\t\t\t\t\t\tApplyPatch(TMP, \"CallbackName\", CallbackName)\n\t\t\t\t\t\t\tApplyPatch(TMP, \"SpecificName\", StateVar.Name)\n\t\t\t\t\t\t\tApplyPatch(TMP, \"NavbarName\", NewNavBarIcon.Name)\n\t\t\t\t\t\t\tApplyPatch(TMP, \"OverlayName\", NewOverlay.Name)\n\t\t\t\t\t\t\tScript = TMP\n\t\t\t\t\t\t\tScript.Enabled= true\n\t\t\t\t\t\tend\n\t\t\t\t\tend\n\t\t\t\tend\n\t\t\t\twarn(\"Setting Made: \"..NewIcon.Name)\n\t\t\tend\n\t\tend\n\tend\n\twarn(\"IconData\")\n\twarn(BuildTable)\n\treturn\nend\n\n--Create pre-defined elements\nwarn(Settings)\nCreateNewElement(Settings, Player:WaitForChild(\"PlayerGui\"):FindFirstChild(\"MenuScreen\"))\n"},"Parent":"ProjectFolder","CLS":"Script","Children":{"Assets":{"NME":"Assets","Parent":"UGUISystem","Properties":{"Parent":"UGUISystem","Name":"Assets","Archivable":"true"},"Children":{"Icons":{"NME":"Icons","Parent":"Assets","Properties":{"Parent":"Assets","Name":"Icons","Archivable":"true"},"Children":{"OverlayIcon":{"NME":"OverlayIcon","Parent":"Icons","Properties":{"Visible":"true","Active":"false","Selectable":"false","Parent":"Icons","Name":"OverlayIcon","AnchorPoint":"0, 0","Archivable":"true","RootLocalizationTable":"nil","NextSelectionUp":"nil","NextSelectionRight":"nil","ZIndex":"2","AutomaticSize":"Enum.AutomaticSize.None","Size":"{0, 755}, {0, 321}","SizeConstraint":"Enum.SizeConstraint.RelativeXY","BorderSizePixel":"0","AutoLocalize":"true","ClipsDescendants":"false","BorderColor3":"0, 0, 0","NextSelectionLeft":"nil","NextSelectionDown":"nil","LayoutOrder":"0","Rotation":"0","Transparency":"0.75","Style":"Enum.FrameStyle.Custom","SelectionImageObject":"nil","Bord