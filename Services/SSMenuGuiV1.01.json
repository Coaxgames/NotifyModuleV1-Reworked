t":"nil","TextWrapped":"false","LineHeight":"1","Active":"false","TextStrokeTransparency":"1","TextTruncate":"Enum.TextTruncate.None","SizeConstraint":"Enum.SizeConstraint.RelativeXY","ZIndex":"1","BorderSizePixel":"0","RichText":"false","AutoLocalize":"true","NextSelectionDown":"nil","Transparency":"0","TextYAlignment":"Enum.TextYAlignment.Center","TextScaled":"false","BackgroundColor3":"0.54902, 0.54902, 0.54902","FontFace":"Font { Family = rbxasset://fonts/families/SourceSansPro.json, Weight = Regular, Style = Normal }","BorderMode":"Enum.BorderMode.Outline","Selectable":"false","Parent":"Interaction","AnchorPoint":"0, 0","Name":"TitleCard","Archivable":"true","TextXAlignment":"Enum.TextXAlignment.Center","NextSelectionRight":"nil","TextTransparency":"0","AutomaticSize":"Enum.AutomaticSize.None","Size":"{0.606266737, 0}, {0.5, 0}","TextStrokeColor3":"0, 0, 0","TextSize":"14","LayoutOrder":"0","ClipsDescendants":"false","BorderColor3":"0, 0, 0","Text":"Label","TextColor3":"0, 0, 0","RootLocalizationTable":"nil","Rotation":"0","Font":"Enum.Font.SourceSans","BackgroundTransparency":"0","Position":"{0.34157148, 0}, {0.238673925, 0}","Visible":"true","MaxVisibleGraphemes":"-1","NextSelectionUp":"nil","SelectionImageObject":"nil"},"Children":{"UICorner":{"NME":"UICorner","Parent":"TitleCard","Properties":{"Parent":"TitleCard","Name":"UICorner","Archivable":"true","CornerRadius":"0, 8"},"Children":[],"CLS":"UICorner"}},"CLS":"TextLabel"},"UICorner":{"NME":"UICorner","Parent":"Interaction","Properties":{"Parent":"Interaction","Name":"UICorner","Archivable":"true","CornerRadius":"0, 8"},"Children":[],"CLS":"UICorner"}},"CLS":"TextButton"},"Image":{"NME":"Image","Parent":"NavBarIcon","Properties":{"Visible":"true","Active":"false","SizeConstraint":"Enum.SizeConstraint.RelativeXY","ZIndex":"1","BorderSizePixel":"0","SliceCenter":"0, 0, 0, 0","ScaleType":"Enum.ScaleType.Stretch","AutoLocalize":"true","NextSelectionDown":"nil","Transparency":"0","BackgroundColor3":"0.54902, 0.54902, 0.54902","ImageTransparency":"0","Selectable":"false","Parent":"NavBarIcon","AnchorPoint":"0, 0","Image":"rbxasset://textures/ui/GuiImagePlaceholder.png","TileSize":"{1, 0}, {1, 0}","ImageRectSize":"0, 0","NextSelectionRight":"nil","Name":"Image","AutomaticSize":"Enum.AutomaticSize.None","Size":"{0.230361909, 0}, {0.599447489, 0}","Archivable":"true","RootLocalizationTable":"nil","ImageColor3":"1, 1, 1","ClipsDescendants":"false","BorderColor3":"0, 0, 0","NextSelectionUp":"nil","LayoutOrder":"0","NextSelectionLeft":"nil","Rotation":"0","ImageRectOffset":"0, 0","BackgroundTransparency":"0","Position":"{0.0531332903, 0}, {0.198894933, 0}","SliceScale":"1","BorderMode":"Enum.BorderMode.Outline","SelectionImageObject":"nil","ResampleMode":"Enum.ResamplerMode.Default"},"Children":{"UICorner":{"NME":"UICorner","Parent":"Image","Properties":{"Parent":"Image","Name":"UICorner","Archivable":"true","CornerRadius":"0, 5"},"Children":[],"CLS":"UICorner"}},"CLS":"ImageLabel"},"Type":{"NME":"Type","Parent":"NavBarIcon","Properties":{"Value":"Button","Parent":"NavBarIcon","Archivable":"true","Name":"Type"},"Children":[],"CLS":"StringValue"},"UICorner":{"NME":"UICorner","Parent":"NavBarIcon","Properties":{"Parent":"NavBarIcon","Name":"UICorner","Archivable":"true","CornerRadius":"0, 8"},"Children":[],"CLS":"UICorner"}},"CLS":"Frame"}},"CLS":"Folder"},"Scripts":{"NME":"Scripts","Parent":"Assets","Properties":{"Parent":"Assets","Name":"Scripts","Archivable":"true"},"Children":{"Attributes":{"NME":"Attributes","Parent":"Scripts","Properties":{"Value":"false","Parent":"Scripts","Archivable":"true","Name":"Attributes"},"Children":[],"CLS":"BoolValue"},"ButtonScriptV2":{"NME":"ButtonScriptV2","Parent":"Scripts","Properties":{"Enabled":"false","LinkedSource":"","Disabled":"true","Parent":"Scripts","Name":"ButtonScriptV2","Archivable":"true","Source":"--RE-WRITE THIS BEFORE WORKING ON ANYTHING ELSE!!!\nlocal Clickable=true\nlocal DP = {\n}\nlocal DL = { --Debug Table\n\t--Local Always Availible Stuff\n\t[\"Startup\"]=false,\n\t[\"PatchWork\"]=false,\n\t[\"ButtonComTX\"]=true,\n\t[\"ButtonComRX\"]=false,\n\t[\"Errors\"]=true,\n\t--___________________\n\t--Custom Stuff\n\t[\"ButtonComTX Interaction\"]=false, --On Event Trigger\n\t[\"ButtonComTX Trigger\"]=true, --On Event Trigger {IF Player Pressed Button}\n\n}\n\n--Setup Coms\nlocal Ev = script.Parent.Parent.Parent.Parent.Parent:FindFirstChild(\"ComPlugin\") --Needs to be repiped\nif Ev==nil then\n\tEv = script.Parent.Parent.Parent:FindFirstChild(\"ComPlugin\") --Search 1 layer further in\nend\n--Get Our Local Instances and such for GUI work\nlocal BN\nlocal ButtonBox = script.Parent:FindFirstChild(\"Interaction\")\nlocal TitleBox = script.Parent:FindFirstChild(\"SettingTitle\")\nlocal IconType = script.Parent:FindFirstChild(\"Type\")\nlocal IconElem = script.Parent\nlocal BoolState= IconType.Value\nif BoolState==\"Bool\" then\n\tBoolState=script.Parent:FindFirstChild(\"BoolState\")\nend\n\n--__________________Functions\n--Debug Function (Not Used yet but is ready to use)\nlocal function DebugLog(MSG : \"The printed Message(Concate BEFORE Sending into function)\", Warn : \"Highlight Message? True|False\", DebugType : \"Tag, in DL Settings set Create your tag there. Then use it here to check if its enabled \")\n\tlocal function Print(MSG1, W) --General Print Function\n\t\tif W then warn(MSG)\n\t\telse print(MSG)\n\t\tend\n\t\treturn\n\tend\n\tif DebugType~=nil then\n\t\tif table.find(DL, DebugType) then if DL[DebugType]==true then \n\t\t\t\tPrint(MSG, Warn) --Debug Setting Limited Print \n\t\t\tend\n\t\tend \n\telse Print(MSG, Warn) --Regular Printing\n\tend\n\treturn\nend\n\n\n\n\n--________________________Script Start\n--Get Attribute\nif DL[\"Startup\"] then\n\twarn(script.Name..\"---------#002----LOCAL Button Debug Data-------------\")\n\twarn(\"Registering Attributes\")\t\nend\nfor i, Obj in pairs(script:GetChildren()) do\n\tfor i2, Atr in pairs(script:GetAttributes()) do\n\t\tif DL[\"Startup\"] then warn(\"Added \"..i2..\" With Value of \", Atr) end\n\t\tif Atr == Obj.Name or i2 == \"SpecificName\" then\n\t\t\tif DL[\"Startup\"] then warn(\"SpecName Set for \", Atr) end\n\t\t\tBN = Atr\n\t\tend\n\tend\n\tif DL[\"Startup\"] then warn(\"Registered Attributes for \", BN) end\nend\n\nlocal function ErrorOut()\n\tIconElem.Visible = false\n\tClickable = false\n\tscript.Enabled = false\n\treturn\nend\n\n--Prechecks\nif Ev==nil then\n\tif DL[\"Errors\"] then warn(ButtonBox.Name, \"Failed to Init, No Coms Detected\") end\n\tErrorOut()\nelseif BN==nil then\n\tif DL[\"Errors\"] or DL[\"Startup\"] then warn(BN, \"Failed to Init, ButtonName not set\") end\n\tErrorOut()\nelse\n\tif DL[\"Errors\"] or DL[\"Startup\"] then warn(BN, \"Started Successfully\") end\t\nend\n\n--Attributes(Add later)\n--local Enabled = script:FindFirstChild(BN):GetAttribute(\"Enabled\")\n--local UserLocked = script:FindFirstChild(BN):GetAttribute(\"UserLocked\")\n\nif Ev.Name == \"ComPlugin\" then\n\tlocal TMP = Ev:FindFirstChild(\"Buttons\")\n\tEv = TMP\n\tEv.Event:Connect(function(ButtonNameIN, DataPacket, Debounce)\n\t\tif Debounce==false and BN==ButtonNameIN then\n\t\t\t--Inline Code Structure\n\t\t\t--if UserLocked==true then --Attributes are only changed on server side, used for hard locks and such\n\t\t\t--\tClickable=false\n\t\t\t--end\n\n\t\t\t--ToDO: Add State(WithColor), add Text, Add Image, Add title Change\n\t\t\tif DataPacket[\"SetSettingVar\"]~=nil then\n\t\t\t\t--if Clickable==true then\n\t\t\t\tscript:FindFirstChild(BN).Value = DataPacket[\"SetSettingVar\"]\n\t\t\t\t--else\n\t\t\t\t--\tif DL[\"ButtonComTX\"] then warn(BN, \"Button Disabled\") end\n\t\t\t\t--end\n\t\t\tend\n\t\t\tif DataPacket[\"StateColor\"]~=nil then\n\t\t\t\tBoolState.BackgroundColor3=DataPacket[\"StateColor\"]\n\t\t\tend\n\t\t\tif DataPacket[\"ElemColor\"]~=nil then\n\t\t\t\tIconElem.BackgroundColor3=DataPacket[\"ElemColor\"]\n\t\t\tend\n\t\t\tif DataPacket[\"Clickable\"]~=nil then\n\t\t\t\tClickable=DataPacket[\"Clickable\"]\n\t\t\tend\n\t\t\tif DataPacket[\"GamepassLimited\"]~=nil then\n\t\t\t\tClickable=false\n\t\t\t\tIconElem.BackgroundColor3 = Color3.new(0.239216, 0.152941, 0.156863)\n\t\t\tend\n\t\t\tif DataPacket[\"RemoveElem\"]==true then\n\t\t\t\tErrorOut()\n\t\t\tend\n\n\t\t\t--Debug\n\t\t\tif DL[\"ButtonComRX\"] then \n\t\t\t\twarn(ButtonNameIN..\" Recieved Event\")\n\t\t\t\twarn(DataPacket)\n\t\t\tend\n\t\tend\n\tend)\t\nelse\n\tif DL[\"Errors\"] then warn(BN, \"Failed to Init, Coms Minconfigured/Error\") end\nend\n\n--Interaction Manager Patches\nlocal BB\nif ButtonBox == nil then\n\tlocal TMP = script.Parent\n\tif DL[\"PatchWork\"] then warn(BN, \"Patching, nil --> \"..TMP.Name) end\n\tButtonBox = TMP\n\tBB = ButtonBox.InputBegan\n\tif DL[\"PatchWork\"] then warn(BN, \"Patched, Main UI Icon was Minconfigured\") end\nelse\n\tif IconType.Value==\"String\" or IconType.Value==\"Number\" then\n\t\tBB = ButtonBox.Parent.InputBegan\n\telse\n\t\tBB = ButtonBox.InputBegan\n\tend\nend\nif ButtonBox~=nil or script:FindFirstChild(ButtonBox)~=nil then\n\tBB:Connect(function(Input)\n\t\tif DL[\"ButtonComTX Interaction\"] then \n\t\t\tprint(BN, Input.UserInputType.Name)\n\t\t\tprint(BN, Input.UserInputState.Name)\n\t\tend\n\t\tif Clickable==false --[[or Enabled==false]] then\n\t\t\tif DL[\"ButtonComTX\"] then warn(\"Button Disabled!\") end\n\t\t\treturn\n\t\tend\n\t\tif Input.UserInputType.Name==\"MouseButton1\" or Input.UserInputType.Name==\"Touch\" then\n\t\t\tif DL[\"ButtonComTX Trigger\"] then \n\t\t\t\tprint(BN, Input.UserInputType.Name)\n\t\t\t\tprint(BN, Input.UserInputState.Name)\n\t\t\tend\n\t\t\tDP={\n\t\t\t\t[\"KeyCode\"]=Input.UserInputType.Name,\n\t\t\t\t[\"State\"]=Input.UserInputState.Name,\n\t\t\t\t[\"ButtonName\"]=BN,\n\t\t\t\t[\"Type\"]=IconType.Value,\n\t\t\t\t[\"NavbarName\"]=script:GetAttribute(\"NavbarName\"),\n\t\t\t\t[\"OverlayName\"]=script:GetAttribute(\"OverlayName\"),\n\t\t\t\t[\"CallbackName\"]=script:GetAttribute(\"CallbackName\"),\n\t\t\t\t[\"VaribleInst\"]=script:FindFirstChild(BN),\n\t\t\t}\n\t\t\tif IconType~=nil then\n\t\t\t\tif IconType.Value==\"String\" or IconType.Value==\"Number\" then\n\t\t\t\t\tDP[\"Text\"]=ButtonBox.Text\n\t\t\t\tend\n\t\t\t\tif IconType.Value==\"Button\" then\n\t\t\t\t\tDP[\"Action\"]=\"Pressed\"\n\t\t\t\tend\n\t\t\t\tif IconType.Value==\"Bool\" then\n\t\t\t\t\tDP[\"Action\"]=\"ToggleBool\"\n\t\t\t\tend\n\t\t\tend\n\t\t\tEv:Fire(BN, DP, true)\n\t\tend\n\tend)\n\nelse\n\tif DL[\"Errors\"] then warn(BN, \"Failed to Init, Main UI Icon Minconfigured/Error/Missing\") end\nend\nif DL[\"Startup\"] then warn(script.Name..\"---------#002----LOCAL Button Debug End-------------\") end\n\n"},"Children":[],"CLS":"LocalScript"}},"CLS":"Folder"}},"CLS":"Folder"},"Settings":{"NME":"Settings","Parent":"UGUISystem","Properties":{"Parent":"UGUISystem","Name":"Settings","Archivable":"true"},"Children":{"TestSettings":{"NME":"TestSettings","Parent":"Settings","Properties":{"Parent":"Settings","Name":"TestSettings","Archivable":"true"},"Children":{"TestBool1":{"NME":"TestBool1","Parent":"TestSettings","Properties":{"Value":"false","Parent":"TestSettings","Archivable":"true","Name":"TestBool1"},"Children":[],"CLS":"BoolValue"},"TestNumber1":{"NME":"TestNumber1","Parent":"TestSettings","Properties":{"Value":"1.2","Parent":"TestSettings","Archivable":"true","Name":"TestNumber1"},"Children":[],"CLS":"NumberValue"},"TestString1":{"NME":"TestString1","Parent":"TestSettings","Properties":{"Value":"","Parent":"TestSettings","Archivable":"true","Name":"TestString1"},"Children":[],"CLS":"StringValue"}},"CLS":"Folder"}},"CLS":"Folder"},"Dest":{"NME":"Dest","Parent":"UGUISystem","Properties":{"Value":"StarterCharacterScripts","Parent":"UGUISystem","Archivable":"true","Name":"Dest"},"Children":[],"CLS":"StringValue"},"Coms":{"NME":"Coms","Parent":"UGUISystem","Properties":{"Parent":"UGUISystem","Name":"Coms","Archivable":"true"},"Children":{"GuiComs":{"NME":"GuiComs","Parent":"Coms","Properties":{"Pa