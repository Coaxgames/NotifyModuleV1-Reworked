--TAG;/n{"CrossPlay":{"DestinationParent":"Services","NME":"CrossPlay","Properties":{"Enabled":"true","LinkedSource":"","Disabled":"false","Parent":"ProjectFolder","Name":"CrossPlay","Archivable":"true","Source":"local HTTP=game:GetService(\"HttpService\")\n--Vars\nClonedPlayersList={}\nlocal LogsService=require(game.Workspace:WaitForChild(\"Services\"):WaitForChild(\"LogService\").ModuleScript)\nlocal Notify=require(script.Parent.notify)\nlocal SyceService=game:GetService(\"MessagingService\")\nlocal Topic=\"Main\"\nlocal PlrChatWatch={\n\t\n}\n--[[wait(8)\nprint(\"Logging\")\nLogsService.AllClients(\"Logged New Registered Player!\", \"NIL\")\nwait(80)]]\n--Functions\ngame.Players.PlayerAdded:Connect(function(Player)\n\tprint(\"Loading\")\n\tPlayer.CharacterAdded:Wait()\n\twait(2)\n\tPlrChatWatch[Player.UserId]={\n\t\t[\"Chats\"]={},\n\t\t[\"TimeLeft\"]=0,--(0.1 * ChatsCount * 5) This is the time to force a plr chat to wait before sending\n\t\t[\"Queue\"]=2--IF they reach this number they are timed out\n\t}\n\t--print(game.JobId)\n\t--print(\"Firing\")\n\n\tlocal DataPacket={\n\t\t[\"GID\"]=tostring(game.JobId),\n\t\t[\"JSONData\"]={\n\t\t\t[\"PlayerPos\"]={\n\t\t\t\t[\"Pos\"]=\"\",\n\t\t\t\t[\"Oreintation\"]=\"\",\n\t\t\t},\n\t\t\t[\"PlayerUID\"]=HTTP:JSONEncode(game.Players[Player.Name].UserId),--Others Servers can use this to Load the avatar, rather than bogging down MessagingService\n\t\t},\n\t\t[\"FlowService\"]={\n\t\t\t[\"Mode\"]=\"Sending\",\n\t\t\t[\"Action\"]=\"RegisterPlayerForCrossPlay\"\n\t\t}\n\t}\n\tLogsService.AllClients(Player, \"Logged New Registered Player!\", DataPacket)\n\tSyceService:PublishAsync(Topic, DataPacket)\nend)\ngame.Players.PlayerRemoving:Connect(function(Player)\n\tprint(\"Removing\")\n\tprint(game.JobId)\n\tlocal DataPacket={\n\t\t[\"GID\"]=tostring(game.JobId),\n\t\t[\"JSONData\"]={\n\t\t\t[\"PlayerPos\"]={\n\t\t\t\t[\"Pos\"]=\"\",\n\t\t\t\t[\"Oreintation\"]=\"\",\n\t\t\t},\n\t\t\t[\"PlayerUID\"]=HTTP:JSONEncode(game.Players[Player.Name].UserId),--Others Servers can use this to Load the avatar, rather than bogging down MessagingService\n\t\t},\n\t\t[\"FlowService\"]={\n\t\t\t[\"Mode\"]=\"Sending\",\n\t\t\t[\"Action\"]=\"DeRegisterPlayerForCrossPlay\"\n\t\t}\n\t}\n\tLogsService.AllClients(Player, \"Logged New Registered Player!\", DataPacket)\n\tSyceService:PublishAsync(Topic, DataPacket)\nend)\nSyceService:SubscribeAsync(Topic, function(DataPacket)\n\tpcall(function()\n\t\tif game.JobId~=DataPacket.Data.GID then\n\t\t\tprint(\"GGG\")\n\t\t\tif DataPacket.Data.FlowService.Mode==\"Sending\" and DataPacket.Data.FlowService.Action==\"Chatting\" then\n\t\t\t\tif DataPacket.FlowService.TargetData==nil or DataPacket.FlowService.TargetData==\"\" or DataPacket.FlowService.TargetData==\"All\" then\n\t\t\t\t\tlocal TargetPlr = DataPacket.Data.FlowService.TargetData\n\t\t\t\t\tlocal Plr=game.Players[TargetPlr]\n\n\n\t\t\t\t\tprint(\"FiringAll\")\n\t\t\t\t\tgame.Players[TargetPlr].PlayerGui.OwnerGUI.Control.IncomingChats:FireAllClients(DataPacket.FlowService.MSG, TargetPlr..\"| All\")\n\t\t\t\telse\n\t\t\t\t\tprint(\"FiringPlr\")\n\t\t\t\t\tfor i, Plr in pairs(game.Players:GetChildren()) do\n\t\t\t\t\t\tif Plr.Name==DataPacket.FlowService.TargetData then\n\t\t\t\t\t\t\tgame.Players[Plr.Name].PlayerGui.OwnerGUI.Control.IncomingChats:FireClient(Plr, DataPacket.FlowService.MSG, DataPacket.FlowService.TargetData..\"| Private\")\n\t\t\t\t\t\tend\n\t\t\t\t\tend\n\t\t\t\t\t--game.Players[DataPacket.FlowService.TargetData].PlayerGui\n\t\t\t\tend\n\t\t\telseif DataPacket.Data.FlowService.Mode==\"Sending\" and DataPacket.Data.FlowService.Action==\"Timeout\" then--Timeout CMD\n\t\t\t\tif DataPacket.FlowService.TargetData==nil or DataPacket.FlowService.TargetData==\"\" or DataPacket.FlowService.TargetData==\"All\" then\n\t\t\t\t\tlocal S1=string.split(DataPacket.Data.FlowService.MSG, \"|\")\n\t\t\t\t\tfor i, Plr in pairs(game.Players:GetChildren()) do\n\t\t\t\t\t\tif Plr.Name==DataPacket.FlowService.TargetData then\n\t\t\t\t\t\t\tPlrChatWatch[Plr.UserId].TimeLeft=S1[2]\n\t\t\t\t\t\t\tNotify.notify(Plr, \"Action Taken(CrossServer)!\", \"An Admin/Owner has Changed your timeout Time, It is now set to \"..PlrChatWatch[Plr.UserId].TimeLeft..\" Seconds\", \"info\", 4)\n\t\t\t\t\t\tend\n\t\t\t\t\tend\n\t\t\t\tend\n\t\t\tend\n\t\t\t--game.Workspace.YourNPC.Humanoid:ApplyDescription(game.Players:GetHumanoidDescriptionFromUserId(12345)) \n\n\t\t\t--DevNote!!\n\t\t--[[\n\t\t\tClone The Flying orb OR Load the player Avatar and apply it here, WE need Pos, Orientation for the orb Player\n\t\t\tOrbs are for Incoming player data only, I dont want to hammer MessageService a shit ton so Replicating Player Pos's\n\t\t\t\n\t\t\tCrossServer Chats are needed between these players, Maybe make a CrossServer Chat GUI(Similar to the chat)\n\t\t\tThen Work on TerrainGen Purchase System, so we can buy terrain\n\t\t\tAlso Work on \"PrivateLand\" Which makes Terrain Non-Purchaseable if a player owns a gamepass\n\t\t\tTheir building and ChunkData will load as Public Terrain, Good for roads, Trains, \n\t\t\t~Prebuilt structures made by players for other players, This also provides Road Systems that i dont need to build\n\t\t\t\n\t\t\tThen Add the Logsystem into TerrainGen for Players to get an idea of what happend if somthing is to fail\n\t\t\tAlso allows Ppl that are intrested in What the server does(Updates, Players Kicked, Log anything)\n\t\t\t\n\t\t\tThen work on the building system\n\t\t\t\n\t\t\t~\n\t\t\tThen work on the Wire system\n\t\t\t\tOR\n\t\t\tThen work on the Logic System\n\t\t\t~\n\t\t\t\n\t\t\tThen work on the Interaction system\n\t\t\t\n\t\t\tAfter those Re-asses the game\n\t\t\t(Also make sure to Convert this into A installer Plugin similar to Notify)\n\t\t\t\n\t\t\tAlso fix Notify!\n\t\t\t\n\t\t\t\n\t\t]]\t\t\n\n\n\t\t\t--LogsService.AllClients(\"DataIgnored; MSG From Same Server!\", DataPacket)\n\t\t\treturn \n\t\tend\n\t\tprint(DataPacket)\n\t\tLogsService.AllClients(\"All\", \"CrossServer DataIN\", DataPacket)\t\n\tend)\nend)\n\n--ServerChats\nlocal function GetSafeText(TextObject)\n\tprint(7)\n\tlocal filteredMessage\n\tlocal success, errorMessage = pcall(function()\n\t\tfilteredMessage = TextObject:GetNonChatStringForBroadcastAsync()\n\tend)\n\tif success then\n\t\treturn filteredMessage\n\telseif errorMessage then\n\t\tprint(\"Error filtering message:\", errorMessage)\n\tend\n\treturn \"false\"\nend\nlocal function CheckForCensor(Plr, MSG)\n\tprint(6)\n\tlocal textObject\n\tlocal success, errorMessage = pcall(function()\n\t\ttextObject = game:GetService(\"TextService\"):FilterStringAsync(MSG, Plr)\n\tend)\n\tif success then\n\t\tlocal SafeText=GetSafeText(textObject)\n\t\tif SafeText==\"false\" then\n\t\t\treturn \"false\"\n\t\telse\n\t\t\treturn SafeText\n\t\tend\n\telseif errorMessage then\n\t\tprint(\"Error generating TextFilterResult:\", errorMessage)\n\tend\n\treturn \"false\"\nend\n--Format\n--[\"Player\"]={\n--\t[\"Chats\"]={},\n--\t[\"TimeLeft\"]=0,--(0.1 * ChatsCount * 2) This is the time to force a plr chat to wait before sending\n--\t[\"Queue\"]=2--IF they reach this number they are timed out\n--}\n\nscript.CMD.Event:Connect(function(BuiltInCMD, CMD, TargetPlr, TargetUID, CustomNMB)\n\tif TargetUID~=nil then\n\t\tif CustomNMB==nil then\n\t\t\tCustomNMB=60--Incase i missed the command then \n\t\tend\n\t\tCustomNMB=tonumber(CustomNMB)\n\t\tif PlrChatWatch[TargetUID]==nil then\n\t\t\tlocal DataPacket2={\n\t\t\t\t[\"GID\"]=tostring(game.JobId),\n\t\t\t\t[\"JSONData\"]={\n\t\t\t\t\t[\"PlayerPos\"]={\n\t\t\t\t\t\t[\"Pos\"]=\"\",\n\t\t\t\t\t\t[\"Oreintation\"]=\"\",\n\t\t\t\t\t},\n\t\t\t\t\t[\"PlayerUID\"]=HTTP:JSONEncode(TargetUID.UserId),--Others Servers can use this to Load the avatar, rather than bogging down MessagingService\n\t\t\t\t},\n\t\t\t\t[\"FlowService\"]={\n\t\t\t\t\t[\"Mode\"]=\"Sending\",\n\t\t\t\t\t[\"Action\"]=\"Timeout\",\n\t\t\t\t\t[\"MSG\"]=CMD..\"|\"..CustomNMB,\n\t\t\t\t\t[\"TargetData\"]=TargetPlr\n\t\t\t\t}\n\t\t\t}\n\t\t\t--Upload That to other Servers\n\t\t\tSyceService:PublishAsync(Topic, DataPacket2)\n\t\t\treturn \"they are not in this Server, CMD sent to Network!\"\n\t\telse \n\t\t\tPlrChatWatch[TargetUID].TimeLeft=CustomNMB\n\t\t\tNotify.notify(game.Players[TargetPlr], \"Action Taken!\", \"An Admin/Owner has Changed your timeout Time, It is now set to \"..PlrChatWatch[TargetUID].TimeLeft..\" Seconds\", \"info\", 4)\n\t\t\treturn \"Timed Player out\"\n\t\tend\n\t\t\n\t\t\n\tend\nend)\n\nfunction ChatSys(Plr, Sender, MSG, ToPlr, SendToNetwork)\n\tprint(Plr.UserId)\n\tprint(Sender)\n\tprint(MSG)\n\tprint(ToPlr)\n\tprint(SendToNetwork)\n\tlocal Color = {\n\t\t[\"R\"]=0, --Clamped To 0-255\n\t\t[\"G\"]=0, --Clamped To 0-255\n\t\t[\"B\"]=255, --Clamped To 0-255\t\n\t}\n\tif MSG==nil or MSG==\"\" or MSG:len() <= 0 or MSG:len() > 300 then\n\t\tColor = {[\"R\"]=255, [\"G\"]=0, [\"B\"]=0}\n\t\twarn(\"Missing MSG, Or length of MSG to small/Big\")\n\t\tNotify.notify(Plr, \"Chat Rule #3\", \"Keep You chat Between 1-300 Letters\", Color, 4)\n\t\treturn \"Missing Chat Contents, Or length of Chat to small/Big (1-300 Allowed)\"\n\tend\n\tprint(\"Checkign HTTP for Legal Text\")\n\tlocal DataPacket2={\n\t\t[\"GID\"]=tostring(game.JobId),\n\t\t[\"JSONData\"]={\n\t\t\t[\"PlayerPos\"]={\n\t\t\t\t[\"Pos\"]=\"\",\n\t\t\t\t[\"Oreintation\"]=\"\",\n\t\t\t},\n\t\t\t[\"PlayerUID\"]=HTTP:JSONEncode(Plr.UserId),--Others Servers can use this to Load the avatar, rather than bogging down MessagingService\n\t\t},\n\t\t[\"FlowService\"]={\n\t\t\t[\"Mode\"]=\"Sending\",\n\t\t\t[\"Action\"]=\"Chatting\",\n\t\t\t[\"MSG\"]=CheckForCensor(Plr.UserId, MSG),\n\t\t\t[\"TargetData\"]=ToPlr\n\t\t}\n\t}\n\tprint(\"DEBUG1\")\n\tif PlrChatWatch[Plr.UserId]==nil then\n\t\t\n\tend\n\t\n\t--Spam Protection\n\t--pcall(function()\n\t\tif PlrChatWatch[Plr.UserId].TimeLeft >= 400 then\n\t\t\tPlrChatWatch[Plr.UserId].TimeLeft=PlrChatWatch[Plr.UserId].TimeLeft+PlrChatWatch[Plr.UserId].Queue\n\t\t\tColor = {[\"R\"]=255, [\"G\"]=0, [\"B\"]=0}\n\t\t\tNotify.notify(Plr, \"Chat Rule #2\", \"Consider yourself Banned for \"..PlrChatWatch[Plr.UserId].TimeLeft..\" seconds,\\nEither an admin set this or you have been spamming alot\", Color, 4)\n\t\t\treturn \"Consider yourself Banned for \"..PlrChatWatch[Plr.UserId].TimeLeft..\" seconds\"\n\t\tend\n\t\tlocal ChatCount=#PlrChatWatch[Plr.UserId].Chats\n\t\t--print(ChatCount)\n\t\tif ChatCount==nil then\n\t\t\tChatCount=0\n\t\tend\n\t\tChatCount+=1\n\t\tPlrChatWatch[Plr.UserId].Chats[ChatCount]=DataPacket2\n\t\t\n\t\tif PlrChatWatch[Plr.UserId].TimeLeft==0 then\n\t\t\tPlrChatWatch[Plr.UserId].TimeLeft=0.1 * ChatCount * 2\n\t\telse\n\t\t\tPlrChatWatch[Plr.UserId].TimeLeft=PlrChatWatch[Plr.UserId].TimeLeft+(0.1 * ChatCount * 2)\n\tend\n\t\t--return\n\t\t--task.wait(PlrChatWatch[Plr.UserId].TimeLeft)\t\n\t--end)\n\tprint(PlrChatWatch[Plr.UserId].TimeLeft)\n\tprint(PlrChatWatch[Plr.UserId].Queue)\n\tprint(PlrChatWatch[Plr.UserId].Chats)\n\tif PlrChatWatch[Plr.UserId].TimeLeft >= PlrChatWatch[Plr.UserId].Queue/3 then\n\t\t--PlrChatWatch[Plr.UserId].TimeLeft=PlrChatWatch[Plr.UserId].TimeLeft--[[[+PlrChatWatch[Plr.UserId].Queue]]]\n\t\tColor = {[\"R\"]=255, [\"G\"]=0, [\"B\"]=0}\n\t\tNotify.notify(Plr, \"Chat Rule #2\", \"Spamming is not allowed!, You have been timed out for \"..PlrChatWatch[Plr.UserId].TimeLeft..\" seconds\", Color, 4)\n\t\treturn \"You have Chatted too much! Cool down ends in \"..PlrChatWatch[Plr.UserId].TimeLeft..\" seconds\"\n\tend\n\t\n\t\n\tif SendToNetwork then\n\t\tlocal Color = {\n\t\t\t[\"R\"]=0, --Clamped To 0-255\n\t\t\t[\"G\"]=255, --Clamped To 0-255\n\t\t\t[\"B\"]=0, --Clamped To 0-255\t\n\t\t}\n\t\tprint(\"DEBUG2\")\n\t\tif ToPlr~=nil then\n\t\t\tLogsService.AllClients(\"All\", Plr.DisplayName..\" Chatted!\", \"Hidden To Protect UserChat\")\t\n\t\telse\n\t\t\tLogsService.AllClients(\"All\", Plr.DisplayName..\" Chatted Globally!\", DataPacket2)\n\t\tend\n\t\t\n\t\t--DEVNOTE Start saving these logs to server Via a ServerScript \n\t\t--Save this Data by Fitting as much data into one key as possible, Update untill full, if full switch to next key(Like Terrain Gen)\n\t\t\n\t\t--Work on Cloning a Element for the new chat made from the Sender and any other MSG\n\t\t--After that work on Cross Server Chatting\n\t\t--Then work on Cross server Sync(Without Animations for now)\n\t\t\n\t\t--Make Chat for others in this server\n\t\tif MSG==nil or MSG==\"\" or MSG:len() <= 0 or MSG:len() > 300 then\n\t\t\tColor = {[\"R\"]=255, [\"G\"]=0, [\"B\"]=0}\n\t\t\twarn(\"Missing MSG, Or length of MSG to small/Big\")\n\t\t\treturn \"Missing Chat Contents, Or length of Chat to small/Big (1-300 Allowed)\"\n\t\tend\n\t\tprint(\"Sending Chat\")\n\t\tif DataPacket2.FlowService.TargetData==nil or DataPacket2.FlowService.TargetData==\"\" or DataPacket2.FlowService.TargetData==\"All\" then\n\t\t\tprint(\"FiringAll\")\n\t\t\tgame.Players[Plr.Name].PlayerGui.OwnerGUI.Control.IncomingChats:FireAllClients(DataPacket2.FlowService.MSG, Plr.Name..\"| All\")\n\t\telse\n\t\t\tprint(\"FiringPlr\")\n\t\t\tfor i, Plr in pairs(game.Players:GetChildren()) do\n\t\t\t\tif Plr.Name==DataPacket2.FlowService.TargetData then\n\t\t\t\t\tgame.Players[Plr.Name].PlayerGui.OwnerGUI.Control.IncomingChats:FireClient(Plr, DataPacket2.FlowService.MSG, DataPacket2.FlowService.TargetData..\"| Private\")\n\t\t\t\tend\n\t\t\tend\n\t\t\t--game.Players[DataPacket.FlowService.TargetData].PlayerGui\n\t\tend\n\t\t\t\n\t\t\n\t\t\n\t\t\n\t\t\n\t\t--Upload That to other Servers\n\t\tSyceService:PublishAsync(Topic, DataPacket2)\n\telse\n\t\tLogsService.Client(Plr, \"Returned Legal Text\", DataPacket2.FlowService.MSG)\n\tend\n\t\n\tNotify.notify(Plr, \"Filter Results Below\", DataPacket2.FlowService.MSG, Color, 4)\n\t--Give the Feedback to the plr\n\treturn DataPacket2.FlowService.MSG\nend\nscript.CSChats.OnServerInvoke = ChatSys\n\n\n--Main Loop\nlocal O9=0\nlocal O9M=10\nwhile true do\n\twait(0.1)\n\t--Update their Chat Limits\n\tfor i, Player in pairs(game.Players:GetChildren()) do\n\t\tif PlrChatWatch[Player.UserId]~=nil then\n\t\t\tlocal ChatCount=#PlrChatWatch[Player.UserId].Chats\n\t\t\tif ChatCount~=nil and ChatCount~=0 then\n\t\t\t\tif PlrChatWatch[Player.UserId].TimeLeft <= PlrChatWatch[Player.UserId].Queue then\n\t\t\t\t\ttable.remove(PlrChatWatch[Player.UserId].Chats, 1)\n\t\t\t\tend\n\t\t\tend\n\t\t\tif PlrChatWatch[Player.UserId].TimeLeft >= 0.01 then\n\t\t\t\tPlrChatWatch[Player.UserId].TimeLeft=PlrChatWatch[Player.UserId].TimeLeft-0.01\n\t\t\t\t--print(PlrChatWatch[Player.UserId].TimeLeft)\n\t\t\tend\n\t\tend\n\tend\n\t--[[if O9 >= O9M then\n\t\tfor i, Player in pairs(game.Players:GetChildren()) do\n\t\t\tlocal PlrInWorkspace\n\t\t\tlocal PlayerParts\n\t\t\tif game.Workspace:FindFirstChild(Player.Name) then\n\t\t\t\tPlrInWorkspace=game.Workspace[Player.Name]\n\t\t\tend\n\t\t\t--Replicate\n\t\t\tif PlrInWorkspace~=nil then\n\t\t\t\tPlrInWorkspace.Archivable=true\n\t\t\t\tlocal TMP=PlrInWorkspace:GetChildren()\n\t\t\t\tPlayerParts=TMP\n\t\t\t\tPlrInWorkspace.Archivable=false\n\t\t\tend\n\n\t\t\t--print(PlayerParts)\n\t\t\t--[[local DataPacket={\n\t\t\t\t[\"GID\"]=tostring(game.JobId),\n\t\t\t\t[\"JSONData\"]={\n\t\t\t\t\t[\"PlayerPos\"]={\n\t\t\t\t\t\t[\"Pos\"]=\"\",\n\t\t\t\t\t\t[\"Oreintation\"]=\"\",\n\t\t\t\t\t},\n\t\t\t\t\t[\"PlayerUID\"]=HTTP:JSONEncode(game.Players[Player.Name].UserId),--Others Servers can use this to Load the avatar, rather than bogging down MessagingService\n\t\t\t\t},\n\t\t\t\t[\"FlowService\"]={\n\t\t\t\t\t[\"Mode\"]=\"Sending\",\n\t\t\t\t\t[\"Action\"]=\"UpdatePlayerCrossPlay\"\n\t\t\t\t}\n\t\t\t}\n\t\t\t--LogsService.AllClients(\"All\", \"Updated CSPlayer Service\", DataPacket)\n\t\t\tgame:GetService(\"RunService\").Heartbeat:Wait()\n\t\tend\n\t\tO9=0\n\telse\n\t\tO9+=1\n\tend]]\nend\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n"},"Parent":"ProjectFolder","CLS":"Script","Children":{"Dest":{"NME":"Dest","Parent":"CrossPlay","Properties":{"Value":"Services","Parent":"CrossPlay","Archivable":"true","Name":"Dest"},"Children":[],"CLS":"StringValue"},"PackageDesc":{"NME":"PackageDesc","Parent":"CrossPlay","Properties":{"Value":"No Description Availible","Parent":"CrossPlay","Archivable":"true","Name":"PackageDesc"},"Children":[],"CLS":"StringValue"},"CMD":{"NME":"CMD","Parent":"CrossPlay","Properties":{"Parent":"CrossPlay","Name":"CMD","Archivable":"true"},"Children":[],"CLS":"BindableEvent"},"CSChats":{"NME":"CSChats","Parent":"CrossPlay","Properties":{"Parent":"CrossPlay","Name":"CSChats","Archivable":"true"},"Children":[],"CLS":"RemoteFunction"}},"PackageInfo":{"PackageNME":"CrossPlay","PackageDESC":"No Description Availible"}},"CommandsModule":{"DestinationParent":"Services","NME":"CommandsModule","Properties":{"Enabled":"true","LinkedSource":"","Disabled":"false","Parent":"ProjectFolder","Name":"CommandsModule","Archivable":"true","Source":"local MyUserIDs={\n\t[\"3570953953\"]=3570953953,\n}\n--Called by chatting $(){};\nlocal CommandsBuiltIn={\n\t[\"Kick\"]=\"Kick\",\n\t[\"Ban\"]=\"Ban\",\n\t[\"UnBan\"]=\"UnBan\",\n\t[\"Timeout\"]=\"Timeout\",\n}\n\nlocal LogsService=require(game.Workspace:WaitForChild(\"Services\"):WaitForChild(\"LogService\").ModuleScript)\nlocal BannedPlayerStore=game:GetService(\"DataStoreService\"):GetDataStore(\"BannedPlayers\", \"V1\")\n\n\n\nlocal function ChatDetected(Plr, MSG, ToPlr)\n\t--pcall(function()\n\t\tprint(MyUserIDs)\n\t\tprint(Plr.UserId)\n\t\tprint(MyUserIDs[tostring(Plr.UserId)])\n\t\tif MyUserIDs[tostring(Plr.UserId)]==Plr.UserId then\n\t\t\tprint(\"Owner\")\n\t\telse\n\t\t\tprint(\"A player fired a Owner only Event, they are hacking...\")\n\t\t\t--This event should ONLY be fired by owner, anyone else firing it will be kicked\n\t\t\tPlr:Kick(\"We arent sure how you fired an event that is not scripted to fire(Its only fired by the owner)... So to be safe we had to kick you\")\n\t\t\treturn\t\n\t\tend\n\t\tlocal CommandLine=string.match(MSG, '$'..\"%b()\"..\"%b{}\"..\"%b[]\"..\"%b()\"..\";\")\n\t\tif CommandLine==nil then\n\t\t\tCommandLine=string.match(MSG, '$'..\"%b()\"..\"%b{}\"..\"%b[]\"..\"%b()\")\n\t\tend\n\t\tif CommandLine==nil then\n\t\t\tCommandLine=string.match(MSG, '$'..\"%b()\"..\"%b{}\"..\"%b[]\")\n\t\tend\n\t\t\n\t\t\n\t--$(Timeout){BotWot7024}[600]<>\n\tif CommandLine~=nil then\n\t\tprint(CommandLine)\n\t\tlocal Command=string.match(CommandLine, \"%b()\")\n\t\tCommand=string.gsub(Command, \"%(\", \"\") Command=string.gsub(Command, \"%)\", \"\")\n\t\tlocal Target=string.match(CommandLine, \"%b{}\")\n\t\tTarget=string.gsub(Target, \"{\", \"\") Target=string.gsub(Target, \"}\", \"\")\n\t\tlocal CustomMSG=string.match(CommandLine, \"%b[]\")\n\t\tCustomMSG=string.gsub(CustomMSG, \"%[\", \"\") CustomMSG=string.gsub(CustomMSG, \"%]\", \"\")\n\t\tprint(Command)\n\t\tprint(Target)\n\t\tprint(CustomMSG)\n\n\t\t--Commands\n\t\tfor i, CMD in pairs(CommandsBuiltIn) do\n\t\t\tprint(i)\n\t\t\tprint(CMD)\n\t\t\tif i==Command then--If matched to A built in command\n\t\t\t\tif CMD==\"Kick\" then--Search For CMD\n\t\t\t\t\tLogsService.AllClients(\"All\", \"Player Kicked By Owner! Banned \"..Target, nil)\n\t\t\t\t\tlocal TargetPlr=game.Players[Target]:Kick(\"Kicked by Owner! Reason; \"..CustomMSG)\n\t\t\t\telseif CMD==\"Ban\" then--Search For CMD\n\t\t\t\t\tLogsService.AllClients(\"All\", \"Player Banned By Owner! Banned \"..Target, nil)\n\t\t\t\t\tlocal TargetPlr=game.Players[Target]:Kick(\"Kicked by Owner! Reason; \"..CustomMSG)\n\t\t\t\t\tBannedPlayerStore:SetAsync(game.Players[Target].UserId, \"Banned from Owner on \"..os.time())\n\t\t\t\telseif CMD==\"UnBan\" then--Search For CMD\n\t\t\t\t\tLogsService.AllClients(\"All\", \"Player Unbanned By Owner! UnBanned \"..Target, nil)\n\t\t\t\t\tBannedPlayerStore:RemoveAsync(game.Players[Target].UserId)\n\t\t\t\telseif CMD==\"Timeout\" then--Search For CMD\n\t\t\t\t\tprint(\"TimingOut Player\")\n\t\t\t\t\tscript.Parent.CrossPlay.CMD:Fire(i, CMD, Target, game.Players[Target].UserId, CustomMSG)\n\t\t\t\telseif CMD==\"ForceDataReset\" then--Search For CMD\n\n\t\t\t\telseif CMD==\"ForceStatReset\" then--Search For CMD\n\n\t\t\t\telseif CMD==\"ForceKill\" then--Search For CMD\n\n\t\t\t\tend\n\t\t\tend\n\t\tend\n\n\n\tend\n\t--end)\nend\n\n\ngame.Players.PlayerAdded:Connect(function(Plr)\n\t--If The player Fires this and is not the owner they will be kicked, I may ban depending on Security\n\tif MyUserIDs[tostring(Plr.UserId)]==Plr.UserId then\n\t\tPlr.Chatted:Connect(function(MSG, ToPlr)\n\t\t\tChatDetected(Plr, MSG, ToPlr)\n\t\t\tif BannedPlayerStore:GetAsync(Plr.UserId)~=nil then\n\t\t\t\tBannedPlayerStore:RemoveAsync(Plr.UserId)\n\t\t\t\tLogsService.Client(Plr, \"UnBanned Owner!\", nil)\n\t\t\tend\n\t\tend)\n\tend\n\tif BannedPlayerStore:GetAsync(Plr.UserId) then\n\t\twait(2)\n\t\tLogsService.AllClients(\"All\", \"Player Kicked By System! \"..Plr.Name..\" Is Banned\", nil)\n\t\tPlr:Kick(\"You are Perma Banned! You can appeal in the Community Group\")\n\tend\nend)"},"Parent":"ProjectFolder","CLS":"Script","Children":{"Dest":{"NME":"Dest","Parent":"CommandsModule","Properties":{"Value":"Services","Parent":"CommandsModule","Archivable":"true","Name":"Dest"},"Children":[],"CLS":"StringValue"},"PackageDesc":{"NME":"PackageDesc","Parent":"CommandsModule","Properties":{"Value":"No Description Availible","Parent":"CommandsModule","Archivable":"true","Name":"PackageDesc"},"Children":[],"CLS":"StringValue"}},"PackageInfo":{"PackageNME":"CommandsModule","PackageDESC":"No Description Availible"}},"LogService":{"DestinationParent":"Services","NME":"LogService","Properties":{"Enabled":"true","LinkedSource":"","Disabled":"false","Parent":"ProjectFolder","Name":"LogService","Archivable":"true","Source":"\nlocal LogEvent=script.Parent:FindFirstChild(\"ServerLogs\")\nlocal DataStore=game:GetService(\"DataStoreService\"):GetDataStore(\"LogServiceV2\", \"V1\")\nlocal KeyCount=1\nlocal IsExausted=false\nlocal ActiveKey\nlocal CanSave=not game:GetService(\"RunService\"):IsStudio()\nCanSave=true\n\nlocal Stack={}\nlocal function AddToStack(SaveTable, KeyCount)\n\tStack[#Stack+1]={\n\t\t[\"Data\"]=SaveTable,\n\t\t[\"Key\"]=KeyCount\n\t}\n\treturn\nend\n\n\nKeyCount=DataStore:GetAsync(0)\nprint(KeyCount)\nActiveKey=KeyCount\nif KeyCount==nil then\n\tKeyCount=1\n\tActiveKey=1\nend\n\nlocal function CheckSize(NonJsonData)\n\tlocal SizeOfData, PercentFull, StorageStat\n\tpcall(function()\n\t\tSizeOfData=game:GetService(\"HttpService\"):JSONEncode(NonJsonData):len()\n\t\tPercentFull=(SizeOfData/4000000)*100\n\t\tStorageStat=SizeOfData..\"/\"..4000000\n\tend)\n\treturn SizeOfData, PercentFull, StorageStat\nend\n\nlocal function SaveLog(SaveTable, Key, IsStackAction)\n\tlocal S1,S2,S3=CheckSize(SaveTable)\n\tlocal Limit=game:GetService(\"DataStoreService\"):GetRequestBudgetForRequestType(Enum.DataStoreRequestType.UpdateAsync)\n\tprint(Limit)\n\tif Limit <= 2510 and not IsExausted then\n\t\tIsExausted=true\n\telseif Limit >= 2550 and IsExausted then\n\t\tIsExausted=false\n\tend\n\t\n\t\n\tif Key~=nil then\n\t\tActiveKey=Key\n\telse\n\t\tActiveKey=KeyCount\n\tend\n\t\n\t--[[if IsExausted and not IsStackAction then\n\t\tprint(\"added to Stack\")\n\t\tAddToStack(SaveTable, #Stack+1)\n\t\tActiveKey=ActiveKey+1\n\t\tKeyCount=ActiveKey\n\t\treturn false\n\telseif IsExausted and IsStackAction then\n\t\tprint(\"Waiting\")\n\t\tKeyCount=ActiveKey\n\t\treturn false\n\telseif not IsExausted and not IsStackAction then\n\t\tprint(\"Key+1; \"..ActiveKey)\n\t\tActiveKey=ActiveKey+1\n\t\t--return false\n\tend]]\n\t\n\tActiveKey=ActiveKey+1\n\tKeyCount=ActiveKey\n\tprint(\"Saving \"..ActiveKey)\n\t--print(CheckSize(SaveTable))\n\tif CanSave then\n\t\tlocal Suc, Fail = pcall(function()\n\t\t\tDataStore:UpdateAsync(ActiveKey, function(Data)\n\t\t\t\tif Data==nil then\n\t\t\t\t\tlocal JSON=game:GetService(\"HttpService\"):JSONEncode(SaveTable)\n\t\t\t\t\treturn JSON\n\t\t\t\telse\n\t\t\t\t\twarn(\"Couldnt Save, Data Already in key \"..ActiveKey)\n\t\t\t\t\t--AddToStack(SaveTable, #Stack)\n\t\t\t\t\tprint(Data)\n\t\t\t\t\treturn Data\n\t\t\t\tend\n\t\t\tend)\n\t\t\tif CanSave then\n\t\t\t\tDataStore:UpdateAsync(0, function(Data)\n\t\t\t\t\tif Data ~= nil then\n\t\t\t\t\t\tif ActiveKey < Data then\n\t\t\t\t\t\t\tActiveKey=Data\n\t\t\t\t\t\t\treturn nil\n\t\t\t\t\t\telseif ActiveKey >= Data then\n\t\t\t\t\t\t\treturn ActiveKey\n\t\t\t\t\t\tend\n\t\t\t\t\telse\n\t\t\t\t\t\treturn ActiveKey\n\t\t\t\t\tend\n\t\t\t\tend)\n\t\t\tend\n\t\tend) \n\t\tif Fail then\n\t\t\twarn(\"Failed Save, Error Occoured \"..ActiveKey)\n\t\t\tKeyCount=ActiveKey\n\t\t\treturn false\n\t\tend\n\t\t--\n\tend\n\tKeyCount=ActiveKey\n\treturn true\nend\n\n--Main Function for this script\nscript.Event.Event:Connect(function(GState, Plr, LogMSG, DataPacket)\n\t--print(\"DEBUG\")\n\t--print(Plr)\n\t--print(\"SavingLog\")\n\tlocal SaveTable={\n\t\t[\"GState\"]=GState,\n\t\t[\"Plr\"]=Plr.UserId,\n\t\t[\"LogMSG\"]=LogMSG,\n\t\t[\"DataPacket\"]=DataPacket\n\t}\n\t--SaveLog(SaveTable, ActiveKey, false)\n\tif GState==\"Global\" then\n\t\tscript.Parent:FindFirstChild(\"ServerLogs\"):FireAllClients(LogMSG, DataPacket)\n\telseif GState==\"Private\" then\n\t\tscript.Parent:FindFirstChild(\"ServerLogs\"):FireClient(Plr, LogMSG, DataPacket)\n\tend\nend)\n\n\n\n--[[\nlocal C=0\nwhile true do\n\t--print(\"------------LOOP------------\")\n\tlocal Limit=game:GetService(\"DataStoreService\"):GetRequestBudgetForRequestType(Enum.DataStoreRequestType.UpdateAsync)\n\t--print(Limit)\n\t--print(Stack)\n\t--C=C+1\n\t--print(\"LOADED\"..C..\": \"..DataStore:GetAsync(C))\n\t--print(\"------------LOOP------------\")\n\t\n\tif Stack[1]~=nil then\n\t\tif Limit >= 2530 then\n\t\t\t--if SaveLog(Stack[#Stack].Data, Stack[#Stack].Key, true) then\n\t\t\t--\ttable.remove(Stack, #Stack)\t\n\t\t\t--end\n\t\tend\n\tend\n\twait(1.1)\nend]]"},"Parent":"ProjectFolder","CLS":"Script","Children":{"Dest":{"NME":"Dest","Parent":"LogService","Properties":{"Value":"Services","Parent":"LogService","Archivable":"true","Name":"Dest"},"Children":[],"CLS":"StringValue"},"PackageDesc":{"NME":"PackageDesc","Parent":"LogService","Properties":{"Value":"No Description Availible","Parent":"LogService","Archivable":"true","Name":"PackageDesc"},"Children":[],"CLS":"StringValue"},"Event":{"NME":"Event","Parent":"LogService","Properties":{"Parent":"LogService","Name":"Event","Archivable":"true"},"Children":[],"CLS":"BindableEvent"},"ModuleScript":{"NME":"ModuleScript","Parent":"LogService","Properties":{"LinkedSource":"","Source":"\nlocal Module = {}\n\nfunction Module.AllClients(Plr, LogMSG, DataPacket)\n\t--print(\"Called1\")\n\tscript.Parent.Event:Fire(\"Global\", Plr, LogMSG, DataPacket)\nend\nfunction Module.Client(Plr, LogMSG, DataPacket)\n\t--print(\"Called2\")\n\tscript.Parent.Event:Fire(\"Private\", Plr, LogMSG, DataPacket)\nend\n\nreturn Module\n","Parent":"LogService","Archivable":"true","Name":"ModuleScript"},"Children":[],"CLS":"ModuleScript"}},"PackageInfo":{"PackageNME":"LogService","PackageDESC":"No Description Availible"}},"DeployToPlayers":{"DestinationParent":"Services","NME":"DeployToPlayers","Properties":{"Enabled":"true","LinkedSource":"","Disabled":"false","Parent":"ProjectFolder","Name":"DeployToPlayers","Archivable":"true","Source":"--Try adding this to PlayerManager, Wasy simpiler than 2 scripts Coordinating Together\nlocal MarketplaceService = game:GetService(\"MarketplaceService\")\nlocal Notify=require(script.Parent.notify)\nlocal MyUserIDs={\n\t[\"3570953953\"]=3570953953,\n\t[\"1\"]=1,\n\t[\"2\"]=2,\n}\nlocal function GetProducts()\n\tlocal GamepassIDs={\n\t\t[227981226]={\n\t\t\t[\"MarketData\"]=MarketplaceService:GetProductInfo(227981226, Enum.InfoType.GamePass)\n\t\t},\n\t}\n\tprint(GamepassIDs)\n\treturn GamepassIDs\nend\n--wait(80)\nlocal PlayerCache1={}\nlocal PlayerCache2={}\n--Main Script\nlocal Blacklisted={\n\t[\"ExeSys\"]={\n\t\t[\"ParentName\"]=\"PlayerGui/OwnerGUI/OffScreenCache\",\n\t\t[\"Instance\"]=game.ReplicatedStorage:WaitForChild(\"DeployToOwner\"):WaitForChild(\"ExeSys\"):Clone()\n\t},\n\t[\"ExecutorButton\"]={\n\t\t[\"ParentName\"]=\"PlayerGui/OwnerGUI/ScriptExecuterUI/Buttons\",\n\t\t[\"Text\"]=\"Executor\",\n\t\t[\"Instance\"]=game.ReplicatedStorage:WaitForChild(\"DeployToOwner\"):WaitForChild(\"Executer(Owner Only)\"):Clone()\n\t},\n\t[\"ScripterEvent\"]={\n\t\t[\"ParentName\"]=\"PlayerGui/OwnerGUI/Control\",\n\t\t[\"Instance\"]=game.ReplicatedStorage:WaitForChild(\"DeployToOwner\"):WaitForChild(\"ScriptEvent(Owner Only)\"):Clone()\n\t},\n}\nlocal DataToDeploy={\n\t[\"CrossServerChats\"]={\n\t\t[\"ParentName\"]=\"PlayerGui/OwnerGUI/ScriptExecuterUI/Buttons\",\n\t\t[\"Text\"]=\"Cross Server Chatting\",\n\t\t[\"Instance\"]=game.ReplicatedStorage:WaitForChild(\"DeployToOwner\"):WaitForChild(\"CrossServerChats\"):Clone(),\n\t\t[\"GID\"]=227981226\n\t},\n\t[\"CSChatSys\"]={\n\t\t[\"ParentName\"]=\"PlayerGui/OwnerGUI/OffScreenCache\",\n\t\t[\"Instance\"]=game.ReplicatedStorage:WaitForChild(\"DeployToOwner\"):WaitForChild(\"CSChatSys\"):Clone(),\n\t\t[\"GID\"]=227981226\n\t},\n}\n\ngame.Players.PlayerAdded:Connect(function(Plr)\n\t--Check for Gamepass's\n\tPlayerCache1={}\n\tfor i, GID in pairs(GetProducts()) do\n\t\tlocal _success, doesPlayerOwnAsset = pcall(MarketplaceService.PlayerOwnsAsset, MarketplaceService, Plr, i)\n\t\tPlayerCache1[i]=doesPlayerOwnAsset\n\t\twait(0.2)\n\tend\n\tPlayerCache2[Plr.UserId]=PlayerCache1\n\n\tprint(\"Player Joined\")\n\t--Plr.CharacterAdded:Wait()\n\tPlr:WaitForChild(\"PlayerGui\")\n\tPlr:WaitForChild(\"PlayerGui\"):WaitForChild(\"OwnerGUI\")\n\t--if MyUserIDs[tostring(Plr.UserId)]==Plr.UserId then--Add in Gamepasses for players to own this stuff\n\tprint(\"Owner Located\")\n\tfor i, Deploy in pairs(DataToDeploy) do\n\t\tlocal S1=string.split(Deploy.ParentName, \"/\")\n\t\tlocal TMP=Plr[S1[1]]\n\t\tfor C, Obj in pairs(S1) do\n\t\t\tif C==1 then\n\t\t\t\t--TMP=S1[2]\n\t\t\telse\n\t\t\t\tTMP=TMP[S1[C]] \n\t\t\t\t--print(TMP)\n\t\t\tend\n\n\t\tend\n\n\t\t--Check for Owned Passes OR OwnerId\n\t\tif PlayerCache2[Plr.UserId][Deploy.GID] or MyUserIDs[tostring(Plr.UserId)]==Plr.UserId then\n\t\t\tprint(\"Deploying \"..Deploy.ParentName..\" to \"..TMP.Name)\n\t\t\tDeploy.Instance.Parent=TMP\n\t\t\t\n\t\t\tif Deploy.Instance:IsA(\"TextButton\") or Deploy.Instance:IsA(\"TextLabel\") or Deploy.Instance:IsA(\"TextBox\") then\n\t\t\t\tDeploy.Instance.Text=Deploy.Text\n\t\t\tend\n\t\t\tif Deploy.Instance:FindFirstChild(\"DeployedScript\")~=nil or Deploy.Instance:FindFirstChild(\"CustomDeployedScript\")~=nil then\n\t\t\t\tif Deploy.Instance.DeployedScript:IsA(\"LocalScript\") then\n\t\t\t\t\tDeploy.Instance.DeployedScript.Enabled=true\n\t\t\t\tend\n\t\t\tend\n\t\telse\n\t\t\twarn(Plr.Name..\" Does not Own \"..Deploy.GID)\n\t\t\t--return\n\t\tend\n\tend\n\n\t--Load Update Logs\n\tlocal GUI=Plr:WaitForChild(\"PlayerGui\"):WaitForChild(\"OwnerGUI\")\n\tlocal OnScreenCache=GUI:WaitForChild(\"ScriptExecuterUI\"):WaitForChild(\"OnScreenCache\")\n\tlocal OffScreenCache=GUI:WaitForChild(\"OffScreenCache\")\n\t--pcall(function()\n\tlocal TargetUI\n\tlocal Vis=false\n\t--Find out where our log Gui is\n\tif OnScreenCache:FindFirstChild(\"UpdateLogs\") then\n\t\tTargetUI=OnScreenCache\n\t\tVis=true\n\telseif OffScreenCache:FindFirstChild(\"UpdateLogs\") then\n\t\tTargetUI=OffScreenCache\n\t\tVis=false\n\telse\n\t\twarn(\"Missing Element Inside of OwnerGUI\")\n\t\treturn\n\tend\n\t\n\t--Load GID's to the Purchase GUI\n\tfor i, GID in pairs(GetProducts()) do\n\t\tlocal Clone232 = Plr.PlayerGui:WaitForChild(\"OwnerGUI\"):FindFirstChild(\"Assets\"):FindFirstChild(\"PurchaseElement\"):Clone()\n\t\tClone232.TargetGUI.Value=i\n\t\tClone232.Name1.Text=GID.MarketData.Name\n\t\tClone232.Cost.Text=GID.MarketData.PriceInRobux\n\t\tif OnScreenCache:FindFirstChild(\"PurchaseSys\") then\n\t\t\tClone232.Parent=OnScreenCache:FindFirstChild(\"PurchaseSys\").Main\n\t\telseif OffScreenCache:FindFirstChild(\"PurchaseSys\") then\n\t\t\tClone232.Parent=OffScreenCache:FindFirstChild(\"PurchaseSys\").Main\n\t\tend\n\t\tClone232.Visible=true\n\tend\n\t\n\t--print(TargetUI:FindFirstChild(\"LogSys\"))\n\tif TargetUI:FindFirstChild(\"UpdateLogs\") then\n\t\t--local NONJSON = HTTP:JSONDecode(UpdateNotes)\n\t\tlocal HTTPJSON=game:GetService(\"HttpService\"):GetAsync(\"https://raw.githubusercontent.com/Coaxgames/UpdateLogs/main/UpdateLogs.json\")\n\t\t--print(HTTPJSON)\n\t\tlocal UpdateNotes\n\t\tlocal suc1, fail1 = pcall(function()\n\t\t\tUpdateNotes=game:GetService(\"HttpService\"):JSONDecode(HTTPJSON)\n\t\tend)\n\t\tif fail1 then\n\t\t\twarn(fail1)\n\t\t\tlocal Clone=OffScreenCache.Parent.Assets.UpdateElement:Clone()\n\t\t\tClone.Parent=TargetUI:FindFirstChild(\"UpdateLogs\")\n\t\t\tClone.Name=1\n\t\t\tClone.Visible=Vis\n\t\t\t--if Vis then Clone.ZIndex=2 else Clone.ZIndex=-2 end\n\t\t\tClone.UpdateName.Text=\"Server Failed to load\"\n\t\t\tClone.UpdateDesc.Text=\"Error code SDTPx01\\n Data Failed to load from gitrepo. This could be a issue from roblox or a rough update log\"\n\t\t\tClone.UpdateDate.Text=\"Failed to Load\"\n\t\t\treturn\n\t\tend\n\t\t--print(UpdateNotes)\n\t\tfor i, Obj in pairs(UpdateNotes) do\n\t\t\tlocal Time=os.date(\"*t\")\n\t\t\tlocal Clone=OffScreenCache.Parent.Assets.UpdateElement:Clone()\n\t\t\tClone.Parent=TargetUI:FindFirstChild(\"UpdateLogs\")\n\t\t\tClone.Name=i\n\t\t\tClone.Visible=Vis\n\t\t\t--if Vis then Clone.ZIndex=2 else Clone.ZIndex=-2 end\n\t\t\tClone.UpdateName.Text=Obj.UpdateName\n\t\t\tClone.UpdateDesc.Text=Obj.UpdateDesc\n\t\t\tClone.UpdateDate.Text=Obj.UpdateDate\n\t\tend\n\telse\n\t\twarn(\"Missing Element Inside of OwnerGUI\")\n\tend\n\t--end)\n\n\n\tprint(\"Finished Deploying Stuff\")\n\t--end\nend)\nscript.PurchaseGP.OnServerEvent:Connect(function(Plr, RGID)\n\t--Check for Gamepass's\n\tif PlayerCache2[Plr.UserId][RGID] or MyUserIDs[tostring(Plr.UserId)]==Plr.UserId then\t\n\t\tlocal Color={[\"R\"]=255, [\"G\"]=0, [\"B\"]=0,}\n\t\tNotify.notify(Plr, \"Already Own Gamepass\", \"You already own this!\", Color, 4)\n\telse\n\t