"Properties":{"Value":"3","Parent":"UserSettings","Archivable":"true","Name":"MaxStack"},"Children":[],"CLS":"IntValue"},"UIPos":{"NME":"UIPos","Parent":"UserSettings","Properties":{"Value":"3","Parent":"UserSettings","Archivable":"true","Name":"UIPos"},"Children":[],"CLS":"NumberValue"}},"CLS":"Folder"},"LocalScript":{"NME":"LocalScript","Parent":"NotifyModuleContainer","Properties":{"Enabled":"true","LinkedSource":"","Disabled":"false","Parent":"NotifyModuleContainer","Name":"LocalScript","Archivable":"true","Source":"\n--[[\n\nThe way i want this to function:\n1: Player clicks on setting button\n2: the Gui frame moves to preset 1 or 2\n3: Notifacations now show up in a different corner\n4: Notify Module will find this obj and Put things into stack, the stack will handle the rest(This script)\n\n]]\n\nlocal USettings=script.Parent.UserSettings\nlocal Assets=script.Parent.Assets\nlocal CurrentPos=USettings.UIPos.Value\nif USettings.UIPos.Value==2 then\n\tlocal S1=string.split(Assets.Presets.UIPos1.Value, \",\")\n\tscript.Parent.OnScreenCache.Position=UDim2.new(S1[1], S1[2], S1[3], S1[4])\n\t--USettings.UIPos.Value=2\n\tscript.Parent.OnScreenCache.Draggable=true\n\tscript.Parent.OnScreenCache.Selectable=true\n\tscript.Parent.OnScreenCache.Active=true\n\tscript.Parent.OnScreenCache.BackgroundTransparency=0.75\nelseif USettings.UIPos.Value==3 then\n\t--Set the Drag GUI pos for later\n\tlocal S1=string.split(Assets.Presets.UIPos2.Value, \",\")\n\tscript.Parent.OnScreenCache.Position=UDim2.new(S1[1], S1[2], S1[3], S1[4])\n\tscript.Parent.OnScreenCache.BackgroundTransparency=1\n\tscript.Parent.OnScreenCache.Draggable=false\n\tscript.Parent.OnScreenCache.Selectable=false\n\tscript.Parent.OnScreenCache.Active=false\n\t--USettings.UIPos.Value=3\nelseif USettings.UIPos.Value==1 then\n\tscript.Parent.OnScreenCache.Draggable=false\n\tscript.Parent.OnScreenCache.Selectable=false\n\tscript.Parent.OnScreenCache.Active=false\n\tscript.Parent.OnScreenCache.BackgroundTransparency=1\n\t--USettings.UIPos.Value=1\nend\n\nlocal function BlinkFunction(Obj)\n\twait(0.2)\n\tObj.BackgroundTransparency=0.75\n\twait(0.15)\n\tObj.BackgroundTransparency=1\n\twait(0.15)\n\tObj.BackgroundTransparency=0.75\n\twait(0.15)\n\tObj.BackgroundTransparency=1\n\treturn\nend\n\nscript.Parent.SettingsButton.InputBegan:Connect(function(Input)\n\t--print(Input.UserInputType)\n\tif Input.UserInputType.Name == Enum.UserInputType.MouseButton1.Name or Input.UserInputType.Name == Enum.UserInputType.Touch.Name then\n\t\tprint(\"-----------------------------------------------\")\n\t\tif USettings.UIPos.Value==1 then\n\t\t\tprint(1)\n\t\t\tlocal S1=string.split(Assets.Presets.UIPos1.Value, \",\")\n\t\t\tscript.Parent.OnScreenCache.Position=UDim2.new(S1[1], S1[2], S1[3], S1[4])\n\t\t\t\n\t\t\tscript.Parent.OnScreenCache.Draggable=true\n\t\t\tscript.Parent.OnScreenCache.Selectable=true\n\t\t\tscript.Parent.OnScreenCache.Active=true\n\t\t\tscript.Parent.OnScreenCache.BackgroundTransparency=0.75\n\t\t\t\n\t\t\tUSettings.UIPos.Value=2\n\t\telseif USettings.UIPos.Value==2 then\n\t\t\tprint(2)\n\t\t\t--Set the Drag GUI pos for later\n\t\t\tscript.Parent.OnScreenCache.Draggable=false\n\t\t\tscript.Parent.OnScreenCache.Selectable=false\n\t\t\tscript.Parent.OnScreenCache.Active=false\n\t\t\t\n\t\t\t--Save Current onScreen Pos\n\t\t\tlocal S1=tostring(script.Parent.OnScreenCache.Position)\n\t\t\tS1=string.gsub(S1, \"{\", \"\")\n\t\t\tS1=string.gsub(S1, \"}\", \"\")\n\t\t\tAssets.Presets.UIPos1.Value=S1\n\t\t\t\n\t\t\tBlinkFunction(script.Parent.OnScreenCache)\n\t\t\t\n\t\t\tUSettings.UIPos.Value=3\n\t\telseif USettings.UIPos.Value==3 then\n\t\t\tprint(3)\n\t\t\tlocal S1=string.split(Assets.Presets.UIPos2.Value, \",\")\n\t\t\tscript.Parent.OnScreenCache.Position=UDim2.new(S1[1], S1[2], S1[3], S1[4])\n\t\t\t\n\t\t\tscript.Parent.OnScreenCache.Draggable=false\n\t\t\tscript.Parent.OnScreenCache.Selectable=false\n\t\t\tscript.Parent.OnScreenCache.Active=false\n\t\t\tBlinkFunction(script.Parent.OnScreenCache)\n\t\t\t\n\t\t\tUSettings.UIPos.Value=1\n\t\telse\n\t\t\tprint(4)\n\t\t\tUSettings.UIPos.Value=1\n\t\tend\n\t\t--print(USettings.UIPos.Value)\n\t\t\n\tend\nend)\n"},"Children":[],"CLS":"LocalScript"},"Dest":{"NME":"Dest","Parent":"NotifyModuleContainer","Properties":{"Value":"StarterGui","Parent":"NotifyModuleContainer","Archivable":"true","Name":"Dest"},"Children":[],"CLS":"StringValue"},"OnScreenCache":{"NME":"OnScreenCache","Parent":"NotifyModuleContainer","Properties":{"Visible":"true","Active":"false","Selectable":"false","Parent":"NotifyModuleContainer","Name":"OnScreenCache","AnchorPoint":"0, 0","Archivable":"true","RootLocalizationTable":"nil","NextSelectionUp":"nil","NextSelectionRight":"nil","ZIndex":"1","AutomaticSize":"Enum.AutomaticSize.None","Size":"{0.355598003, 0}, {0.501652896, 0}","SizeConstraint":"Enum.SizeConstraint.RelativeXY","BorderSizePixel":"0","AutoLocalize":"true","ClipsDescendants":"true","BorderColor3":"0, 0, 0","NextSelectionLeft":"nil","NextSelectionDown":"nil","LayoutOrder":"0","Rotation":"0","Transparency":"0.8500000238418579","Style":"Enum.FrameStyle.Custom","SelectionImageObject":"nil","BorderMode":"Enum.BorderMode.Outline","BackgroundTransparency":"0.8500000238418579","Position":"{0.0101690171, 0}, {0.248108834, 0}","BackgroundColor3":"1, 1, 1"},"Children":{"Notifications":{"NME":"Notifications","Parent":"OnScreenCache","Properties":{"Parent":"OnScreenCache","Name":"Notifications","Archivable":"true"},"Children":{"UIListLayout":{"NME":"UIListLayout","Parent":"Notifications","Properties":{"VerticalAlignment":"Enum.VerticalAlignment.Bottom","FillDirection":"Enum.FillDirection.Vertical","HorizontalAlignment":"Enum.HorizontalAlignment.Center","Name":"UIListLayout","Parent":"Notifications","Padding":"0, 0","Archivable":"true","SortOrder":"Enum.SortOrder.Name"},"Children":[],"CLS":"UIListLayout"}},"CLS":"Folder"}},"CLS":"Frame"},"Assets":{"NME":"Assets","Parent":"NotifyModuleContainer","Properties":{"Parent":"NotifyModule