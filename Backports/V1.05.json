--TAG;/n{"notify":{"NME":"notify","Parent":"Prebuild","CLS":"ModuleScript","Children":{"NotifyModuleContainer":{"DestinationParent":"StarterGui","NME":"NotifyModuleContainer","Parent":"notify","Properties":{"Enabled":"true","AutoLocalize":"true","ResetOnSpawn":"true","IgnoreGuiInset":"false","Name":"NotifyModuleContainer","DisplayOrder":"0","Parent":"notify","RootLocalizationTable":"nil","Archivable":"true","ZIndexBehavior":"Enum.ZIndexBehavior.Sibling"},"Children":{"SettingsButton":{"NME":"SettingsButton","Parent":"NotifyModuleContainer","Properties":{"Visible":"true","Active":"true","SizeConstraint":"Enum.SizeConstraint.RelativeXY","ZIndex":"1","BorderSizePixel":"0","SliceCenter":"0, 0, 0, 0","ScaleType":"Enum.ScaleType.Stretch","AutoLocalize":"true","NextSelectionDown":"nil","AutoButtonColor":"true","Transparency":"0","BackgroundColor3":"1, 1, 1","Parent":"NotifyModuleContainer","Name":"SettingsButton","Archivable":"true","ImageTransparency":"0","Selectable":"true","LayoutOrder":"0","Modal":"false","RootLocalizationTable":"nil","AnchorPoint":"0, 0","Image":"rbxasset://textures/ui/GuiImagePlaceholder.png","TileSize":"{1, 0}, {1, 0}","ImageRectSize":"0, 0","NextSelectionRight":"nil","Selected":"false","AutomaticSize":"Enum.AutomaticSize.None","Size":"{0, 35}, {0, 30}","ImageColor3":"1, 1, 1","HoverImage":"","SelectionImageObject":"nil","ClipsDescendants":"false","BorderColor3":"0, 0, 0","NextSelectionUp":"nil","NextSelectionLeft":"nil","PressedImage":"","Rotation":"0","ResampleMode":"Enum.ResamplerMode.Default","Style":"Enum.ButtonStyle.Custom","Position":"{0.101983599, 0}, {0.87037158, 0}","SliceScale":"1","BorderMode":"Enum.BorderMode.Outline","BackgroundTransparency":"0","ImageRectOffset":"0, 0"},"Children":{"UICorner":{"NME":"UICorner","Parent":"SettingsButton","Properties":{"Parent":"SettingsButton","Name":"UICorner","Archivable":"true","CornerRadius":"0, 8"},"Children":[],"CLS":"UICorner"},"Cache":{"NME":"Cache","Parent":"SettingsButton","Properties":{"Parent